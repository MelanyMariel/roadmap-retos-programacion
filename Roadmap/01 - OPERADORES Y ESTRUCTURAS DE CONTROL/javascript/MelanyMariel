//Operadores aritméticos

let suma= 5+2;
let resta= 1-2;
división= 10/2;
let multiplicación= 4*5;

//Operadores lógicos 
AND

let and1= true && false;  //true
let and2= false && false; //false
let and3= true && true; //true
OR
let or1= true || false;
let or2= false || false;
let or3= true || true; //true
NOT
//INVIERTE EL VALOR
let valor1= !true;
let valaor2= !false;

//OPERADOR DE COMPARACIÓN

let nota1= a;
let nota2= b;

let bool1= a==b;
let bool2= a>b;
let bool3= a >=b;
let bool4= a <=b;
let bool5= a===b;
